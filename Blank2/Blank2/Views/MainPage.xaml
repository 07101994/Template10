<Page
    xmlns:local="using:Blank2.Views"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:vm="using:Blank2.ViewModels"
    xmlns:Common="using:Blank2.Common"
    x:Class="Blank2.Views.MainPage"
    mc:Ignorable="d">

    <Page.DataContext>
        <vm:MainPageViewModel />
    </Page.DataContext>

    <Grid Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">

        <!-- content -->
        <StackPanel Margin="12" HorizontalAlignment="Left">

            <Common:MyRectangle x:Name="Rec0" />
            <Common:MyRectangle x:Name="Rec1" />
            <Common:MyRectangle x:Name="Rec2" Visibility="Collapsed" />
            <Common:MyRectangle x:Name="Rec3" Visibility="Collapsed" />
            <Common:MyRectangle x:Name="Rec4" Visibility="Collapsed" />
            <Common:MyRectangle x:Name="Rec5" />
            <Common:MyRectangle x:Name="Rec6" x:DeferLoadStrategy="Lazy" />
            <Common:MyRectangle x:Name="Rec7" x:DeferLoadStrategy="Lazy" />
            <Common:MyRectangle x:Name="Rec8" x:DeferLoadStrategy="Lazy" />

        </StackPanel>

    </Grid>
</Page>
